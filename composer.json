{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/allowed_formats": "^1.1",
        "drupal/backup_migrate": "^4.0",
        "drupal/bootstrap": "^3.13",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/bootstrap_library": "^1.11",
        "drupal/ckeditorheight": "^1.3",
        "drupal/coffee": "^1.0@beta",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.6.15",
        "drupal/crop": "^1.5",
        "drupal/ctools": "^3.0",
        "drupal/ds": "^3.1",
        "drupal/embed": "^1.0",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_embed": "1.x-dev",
        "drupal/entity_pager": "1.0-beta2",
        "drupal/environment_indicator": "3.6",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "^1.0",
        "drupal/gdoc_field": "^1.1",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/js_confirm_pop_up": "^1.3",
        "drupal/linked_field": "^1.1",
        "drupal/maxlength": "^1.0@beta",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "1.8",
        "drupal/pathauto": "^1.3",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/svg_image": "^1.8",
        "drupal/video_embed_field": "^2.0",
        "drupal/view_mode_page": "^3.1",
        "drupal/views_bootstrap": "3.x-dev",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/views_slideshow": "^4.6",
        "drush/drush": "^9.0.0",
        "twig/twig": "1.38.2",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-core-require-dev": "~8.6.15",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/devel": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
