# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/default/default.services.yml
/web/sites/default/default.settings.php
/web/sites/default/development.services.yml


# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore local storage
/app/

# Ignore theming tools
/web/themes/custom/.sass-cache/
/web/themes/custom/bootsass/.sass-cache/
/web/themes/custom/bootsass/node-modules/
/web/themes/custom/bootsass/scss/
/web/themes/custom/bootsass/config.rb
/web/themes/custom/bootsass/gruntfile.js
/web/themes/custom/bootsass/package-lock.json
/web/themes/custom/bootsass/package.json

# snapshots
/.ddev/snapshots
